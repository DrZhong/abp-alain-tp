{
  "name": "ztt-wms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 8080 -o",
    "build": "ng build --prod --build-optimizer",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:style",
    "e2e": "ng e2e",
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "test-coverage": "ng test --code-coverage --watch=false",
    "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "hmr": "ng serve --host 0.0.0.0 --port 4200 4201 --hmr -e=hmr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.3",
    "@angular/common": "~8.0.3",
    "@angular/compiler": "~8.0.3",
    "@angular/core": "~8.0.3",
    "@angular/forms": "~8.0.3",
    "@angular/platform-browser": "~8.0.3",
    "@angular/platform-browser-dynamic": "~8.0.3",
    "@angular/router": "~8.0.3",
    "@delon/abc": "^8.0.0",
    "@delon/acl": "^8.0.0",
    "@delon/auth": "^8.0.0",
    "@delon/cache": "^8.0.0",
    "@delon/chart": "^8.0.0",
    "@delon/form": "^8.0.0",
    "@delon/mock": "^8.0.0",
    "@delon/theme": "^8.0.0",
    "@delon/util": "^8.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/jquery": "^3.2.12",
    "@types/moment-timezone": "^0.2.34",
    "abp-ng2-module": "^3.1.0",
    "abp-web-resources": "^3.7.0",
    "ajv": "^6.6.2",
    "famfamfam-flags": "^1.0.0",
    "jquery": "^3.3.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.13",
    "ng-zorro-antd": "^8.0.0-rc.2",
    "ngx-ueditor": "^2.1.3",
    "nswag": "^11.17.20",
    "rxjs": "~6.5.2",
    "rxjs-compat": "^6.2.1",
    "screenfull": "^3.3.3",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "tslib": "^1.9.0",
    "tslint-config-prettier": "^1.17.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.3",
    "@angular/cli": "~8.0.6",
    "@angular/compiler-cli": "~8.0.3",
    "@angular/language-service": "~8.0.3",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "less": "~2.7.0",
    "less-bundle-promise": "^1.0.7",
    "less-loader": "~4.0.0",
    "ng-alain": "^8.0.0-rc.1",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.4.5"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:ts",
      "git add"
    ],
    "*.less": [
      "npm run lint:style",
      "git add"
    ],
    "ignore": [
      "src/assets/*"
    ]
  }
}